#include<stdio.h>
int fibo(int n);

int main(){

int a,c;
scanf("%d",&a);
if(a==0){
    printf("please enter correct value of n");
}
else{
c=fibo(a);
printf("%d",c);
}








return 0;
}
int fibo(int n){
if(n==1){
    return 0;
}
else if(n==2){
    return 1;
}
else{
    return fibo(n-1)+fibo(n-2);
}

}
